{
    "position": "top",
    "layer": "top",
      "output": "eDP-1",
    "height": 14,
    "margin-top": 8,
    "margin-bottom": -10,
    "margin-left": 0,
    "margin-right": 0,
    "modules-left": ["custom/launcher", "battery","memory","cpu", "disk", "custom/archivos"],
    "modules-center": ["hyprland/workspaces"],
    "modules-right": ["custom/playerlabel","custom/playerctl","network", "pulseaudio", "clock"],

    // Modules configuration
	
    "custom/image1": {
	    "format": "<span foreground='#FFFFFF' background='#2e2e2e' font='14' rise='-1000'>üì∑</span>",
	    "on-click": "bash -c 'feh -x ~/.config/nvim/nvimcheat.png && sleep 5 && pkill -f feh'"
    },
    "custom/image2": {
	    "format": "<span foreground='#FFFFFF' background='#2e2e2e' font='14' rise='-1000'>üì∑</span>",
	    "on-click": "bash -c 'feh -x ~/.config/nvim/hypercheat.png && sleep 5 && pkill -f feh'"
    },
    "clock": {
    "format": "<span foreground='#6D8895' font='14.5' rise='-1000'>ÔÄó </span>{:%H:%M}",
    "format-alt": "<span foreground='#6D8895' font='14.5' rise='-1000'>ÔóØ </span>{:%A %B de %Y}",
    "tooltip-format": "<tt>{calendar}</tt>",
    "calendar": {
                "mode"          : "month",
                "mode-mon-col"  : 3,
                "on-scroll"     : 1,
                "on-click-right": "mode",
                "format": {
                          "months":     "<span color='#ffead3'><b>{}</b></span>",
                          "days":       "<span color='#ffffff'><b>{}</b></span>",
                          "weeks":      "<span color='#99ffdd'><b>W{}</b></span>",
                          "weekdays":   "<span color='#ffcc66'><b>{}</b></span>",
                          "today":      "<span color='#66FBFF'><b><u>{}</u></b></span>"
                          }
                },
    "actions":  {
                "on-click-right": "mode",
                "on-click-forward": "tz_up",
                "on-click-backward": "tz_down",
                "on-scroll-up": "shift_up",
                "on-scroll-down": "shift_down"
                }
},
	
	"hyprland/workspaces": {
        "active-only": false,
        "all-outputs": true,
        "disable-scroll": false,
        "on-scroll-up": "hyprctl dispatch workspace -1",
        "on-scroll-down": "hyprctl dispatch workspace +1",
		"format": "{icon}",
		"on-click": "activate",
		"format-icons": {
//			"1": "‰∏Ä",
//			"2": "‰∫å",
//			"3": "‰∏â",
//			"4": "Âõõ",
//			"5": "‰∫î",
			"urgent": "ÔÅ™",
			"active": "ÔÑë",
			"default": "ÔÑë"
		},
    "sort-by-number": true
    },
    "custom/playerctl": {
	    "format": "<span foreground='#FFFFFF'  font='14' rise='-1000'>Û∞è•</span>",
	    "return-type": "json",
	    "max-length": 36,
	    "exec": "playerctl -a metadata --format '{\"text\": \"{{artist}} - {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
	    "on-click": "playerctl play-pause",
	    "format-icons": {
		    "Playing": "<span foreground='#FFFFFF'  font='14' rise='-1000' >Û∞è•</span>",
		    "Paused": "<span foreground='#928374'  font='14' rise='-1000' >Û∞êå</span>"
	    }
    },
    "custom/playerlabel": {
	    "format": "<span foreground='#FFFFFF'  font='14' rise='-1000' >{}</span>",
	    "return-type": "json",
	    "max-length": 36,
	    "exec": "playerctl -a metadata --format '{\"text\": \"{{artist}} - {{markup_escape(title)}}\", \"tooltip\": \"{{playerName}} : {{markup_escape(title)}}\", \"alt\": \"{{status}}\", \"class\": \"{{status}}\"}' -F",
	    "on-click-middle": "playerctl play-pause",
	    "on-click": "playerctl previous",
	    "on-click-right": "playerctl next",
    },
    "battery": {
      "states": {
          "good": 95,
          "warning": 30,
          "critical": 15
      },
      "format": "<span foreground='#FFFFFF'  font='14' rise='-1000'>ÔâÄ   {capacity}%</span>",
      "format-charging": "<span foreground='#FFFFFF'  font='14' rise='-1000'>{capacity}% Ôóß</span>",
      "format-plugged": "<span foreground='#FFFFFF'  font='14' rise='-1000'>{capacity}% Ôá¶</span>"
    },

    "memory": {
        "format": "<span foreground='#FFFFFF' font='14' rise='-1000'>Û∞çõ  {}%</span>",
        "format-alt": "<span foreground='#FFFFFF' font='14' rise='-1000'>Û∞çõ {used}/{total} GiB</span>",
        "interval": 5
    },

    "cpu": {
        "format": "<span foreground='#FFFFFF'  font='14' rise='-1000'>Û∞ª† {usage}%</span>",
        "format-alt": "<span foreground='#FFFFFF'  font='14' rise='-1000'>Û∞ª† {avg_frequency} GHz</span>",
        "interval": 5
    },


    "disk": {
      "format": "<span foreground='#FFFFFF' font='14' rise='-1000'>Û∞ãä {}%</span>",
      "format-alt": "<span foreground='#FFFFFF' font='14' rise='-1000'>Û∞ãä {used}/{total} GiB</span>",
      "interval": 5,
      "path": "/mnt/Datos"
    },

	
	"network": {
        	"format-ethernet": "<span foreground='#7aa2f7' font='14.5' rise='-1000'>Ôõø </span>{ipaddr}",
        	"format-linked": "<span foreground='#7aa2f7' font='14.5' rise='-1000'>Ôõø </span>{ifname} (No IP)"
	},

	"tray": {
		"icon-size": 16,
		"spacing": 5
	},

	"backlight": {
        // "device": "acpi_video1",
	"format": "{icon} {percent}%",
        "format-icons": ["Óéç", "Óèì", "Óèë", "Óèè", "Óèé", "Óèç", "Óèä", "Óèà", "Óéõ"],
//	"on-scroll-up":,
//	"on-scroll-down":,
	},

	"pulseaudio": {
		"format": "<span foreground='#6C77BB' font='14.5' rise='-1000'>{icon}</span> {volume}%",
        	"format-muted": "<span font='14.5' rise='-1000'>Û∞ùü </span>Mute",
		"format-icons": {
			"default": ["Û∞ïø", "Û∞ñÄ", "Û∞ïæ"]
		},
		"on-click": "bash ~/.config/scripts/volume mute",
    "on-scroll-up": "bash ~/.config/scripts/volume up",
    "on-scroll-down": "bash ~/.config/scripts/volume down",
    "scroll-step": 5,
		"on-click-right": "pavucontrol"
  },
